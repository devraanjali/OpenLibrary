<body>

    <div>
      <a href='/books/infobook/<%= user.email%>' class="btn btn-success">books i have</a>
      <a href='/books/sharedBooks/<%= user.email%>' class="btn btn-success">books shared</a>
      <a href='/books/borrowedBooks/<%= user.email%>' class="btn btn-success">books borrowed</a>
     
    </div>
    <p><%= NoMatch %></p>
    <% book.forEach(function(book){ %>
        <div class="container col-sm-12 col-lg-6 jumbotron text-center border" id="jumbotron">
          <div>
            <table style="width:95%">
              <tr>
                <th>Name</th>
                <td><%= book.name%></td>
              </tr>
              <tr>
                <th>Author</th>
                <td><%= book.author%></td>
              </tr>
              <tr>
                  <th></th>
                  <td></td>
              </tr>
            </table>
            </div>
            <div><br>
              <p>
                <a href="/books/sharedBooks/bookInfo/<%= user.email %>/<%= book._id%>" class="btn btn-primary">More Info</a>
                             
              </p>
              
            </div>
        </div>
    <% });%>
      </body>